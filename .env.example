# .env.example

# Copy this file to .env and fill in the values for your environment.
# This file documents the environment variables required to run the project.

# Example:
# SECRET_KEY=your-secret-key
# DEBUG=True

# Database settings
DB_HOST=localhost
DB_PORT=5432
DB_NAME=ftracker_db
DB_USER=ftracker_user
DB_PASSWORD=your_db_password

# Application settings
APP_ENV=development
APP_PORT=8000

# Logging
LOG_LEVEL=INFO

# Third-party API keys (uncomment and set as needed)
# API_KEY_SOME_SERVICE=your-api-key-here

# Email settings (if applicable)
# EMAIL_HOST=smtp.example.com
# EMAIL_PORT=587
# EMAIL_USER=your-email@example.com
# EMAIL_PASSWORD=your-email-password
